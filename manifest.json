{
  "manifest_version": 3,
  "name": "TYPO3 - Env Switcher",
  "version": "1.0.0",
  "author": "Andy Meier",
  "description": "Streamline your TYPO3 workflow. Hyperjump to different environments of a TYPO3 site. Get the UID of the current page.",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "tabs"
  ],
  "optional_host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "options_ui": {
    "page": "settings.html",
    "open_in_tab": true
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Open Environment Switcher",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": []
}
